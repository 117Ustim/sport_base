rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Правила для коллекции workoutHistory
    match /workoutHistory/{sessionId} {
      // Разрешить чтение и запись только авторизованным пользователям
      allow read, write: if request.auth != null;
    }
    
    // Правила для остальных коллекций (если нужно)
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
